<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.mypathus.tsgforce.model.TemplateTextBalance"
		table="balance_text_report">
		<meta attribute="class-description">
			This class contains the Balance files detail.
		</meta>
		<id name="id" column="id" type="integer">
			<generator class="native" />
		</id>
		<property name="employer" column="employer" type="string" />
		<property name="balance" column="balance" type="string" />
		<property name="description" column="description" type="string" />
		<property name="homebankingStatus" column="homebankingStatus" type="string" />
		<property name="mobileBankingStatus" column="mobileBankingStatus" type="string" />
		<property name="hasEstatements" column="hasEstatements" type="string" />
		<property name="aText" column="aText" type="string" />
		<property name="holdAmount" column="holdAmount" type="integer" />
		<property name="openDate" column="openDate" type="string" />
		<property name="closeDate" column="closeDate" type="string" />

	</class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="org.mypathus.tsgforce.model.ReportIdentificationHelper" table="report_identification">
      <meta attribute="class-description">
         This class contains the Balance files detail. 
      </meta>
      <id name="id" column="report_id" type="integer">
         <generator class="native"/>
      </id>
      <property name="type" column="file_type" type="string"/>
      <property name="header" column="header" type="string"/>
      <property name="name" column="report_name" type="string"/>
      <property name="headerRow" column="header_row" type="integer"/>
      <property name="dataRow" column="data_row" type="integer"/>
   </class>
</hibernate-mapping>